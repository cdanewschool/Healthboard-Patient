	<fx:Script>
		<![CDATA[
			import components.educationalResources.bookmarkAdd;
			import components.educationalResources.bookmarkRemove;
			
			import events.BookmarkEvent;
			
			import models.Bookmark;
			
			import mx.collections.ArrayCollection;
			import mx.events.IndexChangedEvent;
			import mx.managers.PopUpManager;
			import mx.utils.ObjectUtil;
			
			import spark.components.TitleWindow;
			
			[Embed(source="/images/smallArrowCollapsedBlue.png")]
			[Bindable] public var blueArrow:Class;
			
			[Embed(source="/images/smallArrowCollapsed.png")]
			[Bindable] public var whiteArrow:Class;
			
			[Bindable] public var bookmarks:ArrayCollection = new ArrayCollection
				(
					[
						new Bookmark( "Food & Diet", 1),
						new Bookmark( "Facts About Fat", 2)
					]
				);
			
			[Bindable] private var bookmark:Bookmark;
			
			private var titles:Array = new Array
				(
					"Educational Resources Home",
					"Food & Diet",
					"Facts About Fat",
					"Related Links",
					"Search Results",
					"Diabetes Basics — Symptoms",
					"placeholder",
					"Heartburn/GRED Home",
					"Physical Conditions",
					"Digestive Disorders",
					"Heartburn/GERD",
					"Mental Conditions"
				);
			
			private function displayBookmark( item:Bookmark ):void 
			{
				viewsEducationalResources.selectedIndex = viewsEducationalResourcesMenu.selectedIndex = viewsEducationalResourcesBreadcrumb.selectedIndex = item.index;
			}
			
			private function addBookmark():void 
			{
				if(	viewsEducationalResources.selectedIndex != 4 
					&& viewsEducationalResources.selectedIndex != 6 ) 
				{
					bookmark = new Bookmark( titles[viewsEducationalResources.selectedIndex], viewsEducationalResources.selectedIndex );
					bookmarks.addItemAt( bookmark, 0 );
					
					var myBookmarkAdd:bookmarkAdd = bookmarkAdd(PopUpManager.createPopUp(AppProperties.getInstance().controller.application, bookmarkAdd) as spark.components.TitleWindow);
					myBookmarkAdd.bookmark = bookmark.clone();
					myBookmarkAdd.addEventListener( BookmarkEvent.DELETE, onRemoveBookmark );
					myBookmarkAdd.addEventListener( BookmarkEvent.EDIT, onEditBookmark );
					
					PopUpManager.centerPopUp( myBookmarkAdd );
				}
			}
			
			private function onEditBookmark( event:BookmarkEvent ):void
			{
				if( bookmark.label != event.bookmark.label ) bookmark.label = event.bookmark.label;
			}
			
			private function onRemoveBookmark( event:BookmarkEvent ):void
			{
				bookmarks.removeItemAt( bookmarks.getItemIndex(bookmark) );
				bookmark = null;
			}
			
			private function removeBookmark():void 
			{
				var myBookmarkRemove:bookmarkRemove = bookmarkRemove(PopUpManager.createPopUp(AppProperties.getInstance().controller.application, bookmarkRemove) as spark.components.TitleWindow);
				myBookmarkRemove.bookmark = bookmark;
				myBookmarkRemove.addEventListener( BookmarkEvent.DELETE, onRemoveBookmark );
				
				PopUpManager.centerPopUp(myBookmarkRemove);
			}
			
			private function onViewChange(event:IndexChangedEvent):void
			{
				updateBookmark();
			}
			
			private function updateBookmark():void
			{
				bookmark = getBookmarkByIndex( viewsEducationalResources.selectedIndex );
			}
			
			private function getBookmarkByIndex( index:int ):Bookmark
			{
				for each(var bookmark:Bookmark in bookmarks)
				{
					if( bookmark.index == index )
					{
						return bookmark;
					}
				}
				
				return null;
			}
			
			[Bindable] private var resourceNames:Array = new Array("Diabetes diet","Diabetes symptoms","Diabetes pictures","Diabetes treatment","Diabetes prevention","Heartburn basics","Heartburn remedies","Heartburn during pregnancy","Heartburn relief","Heartburn prevention");
			
		]]>
	</fx:Script>
	
